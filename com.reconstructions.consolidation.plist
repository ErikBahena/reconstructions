<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.reconstructions.consolidation</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/env</string>
        <string>python3</string>
        <string>-m</string>
        <string>reconstructions.main</string>
        <string>consolidate</string>
        <string>--interval</string>
        <string>60</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/erik/Developer/reconstructions/src</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PYTHONPATH</key>
        <string>/Users/erik/Developer/reconstructions/src</string>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/erik/.reconstructions/consolidation_daemon.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/erik/.reconstructions/consolidation_daemon.log</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
